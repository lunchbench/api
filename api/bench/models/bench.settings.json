{
  "kind": "collectionType",
  "collectionName": "benches",
  "info": {
    "name": "Bench",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "description": {
      "type": "text",
      "default": "A lovely bench in a nice location, looking over the nice location"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Bench",
        "Spot",
        "Area"
      ],
      "required": true
    },
    "condition": {
      "type": "enumeration",
      "enum": [
        "spiffing",
        "okay",
        "appalling"
      ],
      "required": true
    },
    "capacity": {
      "type": "integer",
      "min": 1,
      "required": true
    },
    "location": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    }
  }
}
